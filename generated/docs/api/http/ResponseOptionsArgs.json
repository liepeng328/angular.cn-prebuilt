  "contents": "\n\n<article>\n  <div class=\"breadcrumb\">\n    <script type=\"application/ld+json\">\n      {\n        \"@context\": \"http://schema.org\",\n        \"@type\": \"BreadcrumbList\",\n        \"itemListElement\": [\n          \n            {\n            \"@type\": \"ListItem\",\n            \"position\": 1,\n            \"item\": {\n              \"@id\": \"https://angular.io//api\",\n              \"name\": \"API\"\n            }\n          }\n            ,{\n            \"@type\": \"ListItem\",\n            \"position\": 2,\n            \"item\": {\n              \"@id\": \"https://angular.io/api/http\",\n              \"name\": \"@angular/http\"\n            }\n          }\n            ,{\n            \"@type\": \"ListItem\",\n            \"position\": 3,\n            \"item\": {\n              \"@id\": \"https://angular.io/api/http/ResponseOptionsArgs\",\n              \"name\": \"ResponseOptionsArgs\"\n            }\n          }\n        ]\n      }\n      </script>\n    \n      \n      <a href=\"/api\">API</a>\n    \n      /\n      <a href=\"api/http\">@angular/http</a>\n    \n  </div>\n  <header class=\"api-header\">\n    <label class=\"api-status-label deprecated\">deprecated</label>\n    \n    \n    <label class=\"api-type-label interface\">interface</label>\n    <h1 id=\"responseoptionsargs\"><a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/http/ResponseOptionsArgs#responseoptionsargs\"><i class=\"material-icons\">link</i></a>\n      ResponseOptionsArgs\n    </h1>\n    <span class=\"version\"></span>\n  </header>\n  <div class=\"page-actions\">\n      <a href=\"api/http/ResponseOptionsArgs#\"><label class=\"raised page-label\"><i class=\"material-icons\">mode_edit</i>suggest edits</label></a>\n      <a href=\"https://github.com/angular/angular/tree/6.0.0-beta.5/packages/http/src/interfaces.ts#L64-L78\"><label class=\"raised page-label\"><i class=\"material-icons\">code</i>view source</label></a>\n  </div